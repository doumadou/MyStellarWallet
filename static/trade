	<script type="text/javascript">
                function refreshOffer(){
                    $.ajax({
                         type:"Post",
                         //后台获取数据的函数，注意当对该页面重命名时，
                          //必须手动更改该选项
                           url: "/refreshOffer",
							data: {{.user.UserName}},
                           contentType:"application/json; charset=utf-8",
                           dataType:"json",
                           //成功获取数据
                           success:function (data){
							if (data.Ret == 0)
							{
            					var tab=document.getElementById("my_bids"); 
            					var rows=tab.rows.length; 
								for(var i=1;i<rows;i++){
        							tab.deleteRow(1);
    							}
            					var tab=document.getElementById("my_asks"); 
            					var rows=tab.rows.length; 
								for(var i=1;i<rows;i++){
        							tab.deleteRow(1);
    							}
            					for(var i=0;i<data.Data._embedded.records.length && i <= 10;i++){ 
									if ( data.Data._embedded.records[i].selling.asset_type == "native" ) {
            					    	var tab=document.getElementById("my_asks"); 
            					    	var rows=tab.rows; 
            					    	var row1=tab.insertRow(rows.length); 
            					    	row1.insertCell(0).innerHTML="&nbsp;"+ (data.Data._embedded.records[i].price); 
            					    	row1.insertCell(1).innerHTML="&nbsp;"+ ((data.Data._embedded.records[i].price) * data.Data._embedded.records[i].amount);
            					    	row1.insertCell(2).innerHTML="&nbsp;"+ data.Data._embedded.records[i].amount;
            					    	row1.insertCell(3).innerHTML="<a href='javascript:void(0);' onclick='cancelOffer("+ data.Data._embedded.records[i].id + ")'>撤消</a>";
									} else {
            					    	var tab=document.getElementById("my_bids"); 
            					    	var rows=tab.rows; 
            					    	var row1=tab.insertRow(rows.length); 
            					    	row1.insertCell(0).innerHTML="&nbsp;"+ ( 1 / data.Data._embedded.records[i].price); 
            					    	row1.insertCell(1).innerHTML="&nbsp;"+ (data.Data._embedded.records[i].amount * data.Data._embedded.records[i].price);
            					    	row1.insertCell(2).innerHTML="&nbsp;"+ data.Data._embedded.records[i].amount;
            					    	row1.insertCell(3).innerHTML="<a href='javascript:void(0);' onclick='cancelOffer("+ data.Data._embedded.records[i].id + ")'>撤消</a>";
									}
            					}
								alert("发送成功");
							} else {
								alert(data.Reason);
							}
                           },
                           //显示错误                                       
                           error:function (err){
                            alert(err + "调用后台程序出现错误，请尝试刷新！");
                        }
                     });
                }   
                function refreshBook(){
                    $.ajax({
                         type:"Post",
                         //后台获取数据的函数，注意当对该页面重命名时，
                          //必须手动更改该选项
                           url: "/refreshBook",
							data:JSON.stringify({"issuer":{{.issuer}},
								"code":{{.code}},
							}),
                           contentType:"application/json; charset=utf-8",
                           dataType:"json",
                           //成功获取数据
                           success:function (data){
							if (data.Ret == 0)
							{
            					var tab=document.getElementById("book_bids"); 
            					var rows=tab.rows.length; 
								for(var i=1;i<rows;i++){
        							tab.deleteRow(1);
    							}
            					var tab=document.getElementById("book_asks"); 
            					var rows=tab.rows.length; 
								for(var i=1;i<rows;i++){
        							tab.deleteRow(1);
    							}
            					for(var i=0;i<data.Data.bids.length && i <= 10;i++){ 
            					    var tab=document.getElementById("book_bids"); 
            					    var rows=tab.rows; 
            					    var row1=tab.insertRow(rows.length); 
            					    row1.insertCell(0).innerHTML="&nbsp;"+ data.Data.bids[i].price; 
            					    row1.insertCell(1).innerHTML="&nbsp;"+ (data.Data.bids[i].amount / data.Data.bids[i].price);
            					    row1.insertCell(2).innerHTML="&nbsp;"+ data.Data.bids[i].amount;
            					}
            					for(var i=0;i<data.Data.asks.length && i <= 10;i++){ 
            					    var tab=document.getElementById("book_asks"); 
            					    var rows=tab.rows; 
            					    var row1=tab.insertRow(rows.length); 
            					    row1.insertCell(0).innerHTML="&nbsp;"+ data.Data.asks[i].price; 
            					    row1.insertCell(1).innerHTML="&nbsp;"+ data.Data.asks[i].amount;
            					    row1.insertCell(2).innerHTML="&nbsp;"+ (data.Data.asks[i].price * data.Data.asks[i].amount);
            					}
								alert("发送成功");
							} else {
								alert(data.Reason);
							}
                           },
                           //显示错误                                       
                           error:function (err){
                            alert(err + "调用后台程序出现错误，请尝试刷新！");
                        }
                     });
                }   

                function createoffer(type){
                    $.ajax({
                         type:"Post",
                         //后台获取数据的函数，注意当对该页面重命名时，
                          //必须手动更改该选项
                           url: "/createOffer",
							data:JSON.stringify({"issuer":{{.issuer}},
								"code":{{.code}},
								"price": type == "BUY" ? $("#buy_price").val() : $("#sell_price").val(),
								"amount": type == "BUY" ? $("#buy_amount").val() : $("#sell_amount").val(),
								"type": type,
							}),
                           contentType:"application/json; charset=utf-8",
                           dataType:"json",
                           //成功获取数据
                           success:function (data){
							if (data.Ret == 0)
							{
								alert("发送成功");
							} else {
								alert(data.Reason);
							}
                           },
                           //显示错误                                       
                           error:function (err){
                            alert(err + "调用后台程序出现错误，请尝试刷新！");
                        }
                     });
                }   

                function cancelOffer(offerid){
                    $.ajax({
                         type:"Post",
                         //后台获取数据的函数，注意当对该页面重命名时，
                          //必须手动更改该选项
                           url: "/cancelOffer",
							data:JSON.stringify({"issuer":{{.issuer}},
								"code":{{.code}},
								"price": "0.0001",
								"amount": "0.0001",
								"iD": offerid,
							}),
                           contentType:"application/json; charset=utf-8",
                           dataType:"json",
                           //成功获取数据
                           success:function (data){
							if (data.Ret == 0)
							{
								refreshOffer();
							} else {
								alert(data.Reason);
							}
                           },
                           //显示错误                                       
                           error:function (err){
                            alert(err + "调用后台程序出现错误，请尝试刷新！");
                        }
                     });
                }   


	</script>

</html>
